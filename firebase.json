{
  "hosting": {
    "site": "fortamazing-7a27c",
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "www.fortamazing.com",
        "destination": "https://fortamazing.com",
        "type": 301
      }
    ]
    
  },
  "functions": {
    "source": "functions",
    "codebase": "default",
    "runtime": "nodejs18",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
  }
}
